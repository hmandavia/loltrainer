{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "object-literal-sort-keys": false,
    "quotemark": true,
    "member-access": false
  }
}